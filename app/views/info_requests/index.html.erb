<h1>Listing info_requests</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Institution</th>
      <th>Title</th>
      <th>Phone</th>
      <th>Comments</th>
      <th>Sankey</th>
      <th>Degree mapping</th>
      <th>Analytic dashboards</th>
      <th>Datamart</th>
      <th>Custom dashboards</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @info_requests.each do |info_request| %>
      <tr>
        <td><%= info_request.name %></td>
        <td><%= info_request.email %></td>
        <td><%= info_request.institution %></td>
        <td><%= info_request.title %></td>
        <td><%= info_request.phone %></td>
        <td><%= info_request.comments %></td>
        <td><%= info_request.sankey %></td>
        <td><%= info_request.degree_mapping %></td>
        <td><%= info_request.analytic_dashboards %></td>
        <td><%= info_request.datamart %></td>
        <td><%= info_request.custom_dashboards %></td>
        <td><%= link_to 'Show', info_request %></td>
        <td><%= link_to 'Edit', edit_info_request_path(info_request) %></td>
        <td><%= link_to 'Destroy', info_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Info request', new_info_request_path %>
